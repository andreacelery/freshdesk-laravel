{
    "name": "andreacelery/freshdesk-laravel",
    "description": "Laravel package for the Freshdesk API (v2)",
    "homepage": "https://github.com/andreacelery/freshdesk-laravel",
    "keywords": ["freshdesk", "customer service", "support", "help desk", "api", "laravel"],
    "type": "laravel-package",
    "require": {
        "andreacelery/freshdesk-php-sdk": "0.3.*",
        "illuminate/support": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "laravel/laravel": "~5",
        "laravel/lumen": "~5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Andrea Kyurchiev",
            "email": "andrea@celery.com",
            "homepage": "http://celeryandrea.github.io/"
        }
    ],
    "minimum-stability": "dev",
    "autoload" : {
        "psr-4": {
            "Andreacelery\\Laravel\\Freshdesk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Andreacelery\\Laravel\\Freshdesk\\Test\\": "tests/"
        }
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1-dev"
        }
    }
}
